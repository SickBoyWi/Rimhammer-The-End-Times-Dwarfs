<Defs>

	<!-- Food -->
	<RecipeDef>
		<defName>RH_TET_Dwarf_Stonebread</defName>
		<label>make stonebread</label>
		<description>Make stonebread, a hearty bread made from stoneroot. It doesn't taste amazing, but provides great nutrition and lasts a long time without refrigeration.</description>
		<jobString>Making stonebread.</jobString>
		<workAmount>700</workAmount>
		<workSpeedStat>CookSpeed</workSpeedStat>
		<requiredGiverWorkType>Cooking</requiredGiverWorkType>
		<effectWorking>Cook</effectWorking>
		<soundWorking>Recipe_CookMeal</soundWorking>
		<allowMixingIngredients>false</allowMixingIngredients>
		<ingredientValueGetterClass>IngredientValueGetter_Nutrition</ingredientValueGetterClass>
		<ingredients>
			<li>
				<filter>
					<thingDefs>
						<li>RH_TET_Dwarf_RawStoneroot</li>
					</thingDefs>
				</filter>
				<count>.5</count><!-- game multiplies this by 20 -->
			</li>
		</ingredients>
		<fixedIngredientFilter>
			<thingDefs>
				<li>RH_TET_Dwarf_RawStoneroot</li>
			</thingDefs>
		</fixedIngredientFilter>
		<recipeUsers>
			<li>Campfire</li>
			<li>ElectricStove</li>
			<li>FueledStove</li>
			<li>RH_TET_Dwarfs_Hearth</li>
		</recipeUsers>
		<products>
			<RH_TET_Dwarf_Stonebread>1</RH_TET_Dwarf_Stonebread>
		</products>
		<workSkill>Cooking</workSkill>
		<skillRequirements>
			<Cooking>6</Cooking>
		</skillRequirements>
		<researchPrerequisite>RH_TET_Dwarf_Stonebread</researchPrerequisite>
	</RecipeDef>

