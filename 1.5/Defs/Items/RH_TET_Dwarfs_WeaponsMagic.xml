<?xml version="1.0" encoding="utf-8" ?>
<Defs>

	<ThingDef ParentName="BaseWeapon">
		<defName>RH_TET_Dwarfs_MagicWeapon_GrungniPickaxe</defName>
		<label>Grungni's Pickaxe</label>
		<description>A pickaxe master crafted in the name of the ancestor god Grungni. Having been given the blessing and magic of the god of mining and stonework, it makes mining a breeze, and is very effective in combat.</description>
		<tickerType>Normal</tickerType>
		<graphicData>
			<texPath>Things/Item/Weapons/Melee/RH_TET_Dwarfs_GrungiPickAxe</texPath>
			<graphicClass>Graphic_Single</graphicClass>
			<drawSize>(1.5)</drawSize>
		</graphicData>
		<techLevel>Medieval</techLevel>
		<statBases>
			<DeteriorationRate>0</DeteriorationRate>
			<Mass>2</Mass>
			<MarketValue>4000</MarketValue>
			<Flammability>0</Flammability>
		</statBases>
		<smeltable>false</smeltable>
		<thingCategories Inherit="False">
			<li>RH_TET_DwarfsWeaponsMagic</li>
			<li>RH_TET_Dwarfs_WeaponsMelee</li>
		</thingCategories>
		<comps>
			<li Class="SickCompOversizedWeapon.CompProperties_OversizedWeapon">
				<verticalFlipNorth>true</verticalFlipNorth>
				<angleAdjustmentWest>45</angleAdjustmentWest>
				<angleAdjustmentEast>0</angleAdjustmentEast>
				<northOffset>(0.20,0,0.2)</northOffset>
				<eastOffset>(-.05,0,0.1)</eastOffset>
				<westOffset>(0,0,0.1)</westOffset>
				<southOffset>(-0.30,0,0.05)</southOffset>
			</li>
			<li>
				<compClass>CompQuality</compClass>
			</li>
		</comps>
		<equippedStatOffsets>
			<MiningSpeed>0.33</MiningSpeed>
		</equippedStatOffsets>
		<weaponTags>
			<li>RH_TET_Dwarfs_Magic_Thane</li>
		</weaponTags>
		<equippedAngleOffset>20</equippedAngleOffset>
		<thingSetMakerTags>
			<li>Dwarfs_MagicWeapons</li>
		</thingSetMakerTags>
		<tools>
			<li>
				<label>point</label>
				<capacities>
					<li>Stab</li>
				</capacities>
				<power>42</power>
				<cooldownTime>2.0</cooldownTime>
				<armorPenetration>1.20</armorPenetration>
			</li>
			<li>
				<label>handle</label>
				<capacities>
					<li>Blunt</li>
				</capacities>
				<power>17</power>
				<cooldownTime>2</cooldownTime>
				<surpriseAttack>
					<extraMeleeDamages>
						<li>
							<def>Stun</def>
							<amount>15</amount>
						</li>
					</extraMeleeDamages>
				</surpriseAttack>
			</li>
		</tools>
	</ThingDef>

